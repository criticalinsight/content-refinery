{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "strict": true,
        "skipLibCheck": true,
        "lib": [
            "ESNext"
        ],
        "types": [
            "@cloudflare/workers-types/2023-07-01"
        ],
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "isolatedModules": true
    },
    "include": [
        "src/**/*"
    ]
}